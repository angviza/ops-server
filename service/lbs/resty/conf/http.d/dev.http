upstream logistics {
    server 127.0.0.1:8080 backup;
    server 127.0.0.1:8081;
}

server {
    listen 80 default;
    server_name _;

    #charset koi8-r;
    #access_log  /var/log/nginx/host.access.log  main;

    proxy_redirect off;
    proxy_set_header Host $host;
    proxy_set_header X-Real-IP $remote_addr;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    client_max_body_size 300m;
    client_body_buffer_size 300m;
    proxy_connect_timeout 90;
    proxy_send_timeout 90;
    proxy_read_timeout 90;
    proxy_buffer_size 100m;
    proxy_buffers 4 100m;
    proxy_busy_buffers_size 100m;
    proxy_temp_file_write_size 100m;


    root /www/dev.idflc.cn;
    index index.html index.htm;


    gzip on;
    gzip_static on;
    gzip_proxied any;
    gzip_types
    text/css
    text/javascript
    text/xml
    text/plain
    image/x-icon
    application/javascript
    application/x-javascript
    application/json;
    gzip_http_version 1.0;
    gzip_min_length 10k;
    gzip_buffers 4 16k;
    gzip_comp_level 3;

    include /etc/nginx/conf.d/dev.cn/*.conf;
    location / {
        add_header 'Access-Control-Allow-Origin' '*';
        add_header 'Access-Control-Allow-Credentials' 'true';
        add_header 'Access-Control-Allow-Methods' 'GET, POST, PUT, DELETE, PUTCH, OPTIONS';

        try_files $uri $uri/ /index.html;

        add_header Cross-Origin-Opener-Policy 'same-origin';
        add_header Cross-Origin-Embedder-Policy 'require-corp';

        if ($request_filename ~* .*\.(ico|css|js|gif|jpeg|jpg|png|woff|ttf|otf|svg|woff2|eot|json)$) {
            expires 30d;
            #            add_header Cache-Control "public, no-transform";  这句影响add_header
        }

        if ($request_filename ~* .*\.(?:htm|html)$) {
            add_header Cache-Control "no-cache";
        }
    }


    location /api/logistics/ {
        proxy_pass http://logistics/logistics/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "Upgrade";
        proxy_set_header X-Real-IP $remote_addr;
    }

    location /eventbus/ {
        proxy_pass http://192.168.1.9:8090/eventbus/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "Upgrade";
        proxy_set_header X-Real-IP $remote_addr;
    }

    location /api/city/ {
        proxy_pass http://127.0.0.1:8083/logistics/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "Upgrade";
        proxy_set_header X-Real-IP $remote_addr;
    }

    location /bim/ {
        proxy_pass http://192.168.1.9:9000/;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "Upgrade";
        proxy_set_header X-Real-IP $remote_addr;
    }

    location /fastflow-admin/ {
        proxy_pass http://127.0.0.1:8082/fastflow-admin/;
    }
    location /fastflow-admin2/ {
        proxy_pass http://127.0.0.1:8084/fastflow-admin/;
    }
}